<div
  class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom"
>
  <h1 class="h2">K2Ts Tested</h1>
  <div class="btn-toolbar mb-2 mb-md-0">
    <div class="btn-group me-2">
      <button
        onclick="exportTableToExcel('table')"
        type="button"
        class="btn btn-sm btn-outline-secondary"
      >
        Export
      </button>
    </div>
  </div>
</div>
<div class="table-responsive">
  <caption>
    List of K2Ts Tested
  </caption>
  <table
    class="table table-striped table-sm table-hover align-middle"
    id="table"
  >
    <thead>
      <tr>
        <th>Date Tested</th>
        <th>Serial Number</th>
        <th>Tester</th>
        <th>Result</th>
        <th>Comments</th>
      </tr>
    </thead>
    <tbody>
      <% k2ts.forEach(function(k2t){ %>
      <tr>
        <td style="width: 10%"><%= k2t.testedDate %></td>
        <td style="width: 10%"><%= k2t.assetId %></td>
        <td style="width: 10%"><%= k2t.tester %></td>
        <td><%= k2t.result %></td>
        <td><%= k2t.comments %></td>
      </tr>
      <% }) %>
    </tbody>
  </table>
</div>
